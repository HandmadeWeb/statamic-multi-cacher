{
    "name": "michaelr0/statamic-multi-cacher",
    "license": "MIT",
    "description": "",
    "keywords": [
        "statamic",
        "statamic-addon",
        "statamic-plugin",
        "statamic-cache"
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0",
        "statamic/cms": "^3.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.18",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "autoload": {
        "psr-4": {
            "Michaelr0\\StatamicMultiCacher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Michaelr0\\StatamicMultiCacher\\Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic Multi Cacher",
            "description": "Statamic Multi Cacher addon"
        },
        "laravel": {
            "providers": [
                "Michaelr0\\StatamicMultiCacher\\ServiceProvider"
            ]
        }
    }
}
