{
    "name": "handmadeweb/statamic-multi-cacher",
    "license": "MIT",
    "description": "",
    "keywords": [
        "statamic",
        "statamic-addon",
        "statamic-plugin",
        "statamic-cache"
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "statamic/cms": "^3.1"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "autoload": {
        "psr-4": {
            "HandmadeWeb\\StatamicMultiCacher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HandmadeWeb\\StatamicMultiCacher\\Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic Multi Cacher",
            "description": "Statamic Multi Cacher addon"
        },
        "laravel": {
            "providers": [
                "HandmadeWeb\\StatamicMultiCacher\\ServiceProvider"
            ]
        }
    }
}
